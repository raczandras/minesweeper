<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LaunchController">
  <children>
    <AnchorPane maxHeight="200.0" maxWidth="200.0" minHeight="200.0" minWidth="200.0" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="easyButton" layoutX="45.0" layoutY="0.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" onAction="#easySelected" text="Easy" />
            <Button fx:id="normalButton" layoutX="45.0" layoutY="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" onAction="#normalSelected" text="Normal" />
            <Button fx:id="hardButton" layoutX="45.0" layoutY="80.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" onAction="#hardSelected" text="Hard" />
            <Button fx:id="leaderboardButton" layoutX="45.0" layoutY="120.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" onAction="#leaderboardSelected" text="Leaderboard" />
            <Button fx:id="exitButton" layoutX="45.0" layoutY="160.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" onAction="#exitSelected" text="Exit" />
      </children>
    </AnchorPane>
  </children>
</VBox>
